                "data_type": "int",
                "is_primary_key": true,
                "constraints": {
                    "min_value": 200,
                    "max_value": 1000,
                    "greater_than": 0,
                    "less_than": 300
                }
            },
            {
                "name": "name",
                "data_type": "string",
                "constraints": {
                    "null_ratio": 10,
                    "length":  10,
                    "lowercase": false,
                    "uppercase": false,
                    "regular_expr": null

                }
            },
            {
                "name": "flag",
                "data_type": "boolean"
            }
        ],
        "total_rows": 100
    },
    "Orders": {
        "columns": [
            {
                "name": "id",
                "data_type": "int",
                "is_primary_key": true
            },
            {
                "name": "client_id",
                "data_type": "int",
                "foreign_key": {
                    "table_name": "Client",
                    "column_name": "id",
                    "relation_type": "one_to_one"
                }
            },
            {
                "name": "order_date",
                "data_type": "date",
                "constraints": {
                    "null_ratio": 5,
                    "min_date": "2024-03-01",
