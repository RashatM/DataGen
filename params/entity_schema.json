                    "date_format": "%Y.%m.%d"
                }
            }
        ],
        "total_rows": 30
    },
    "Products": {
        "columns": [
            {
                "name": "id",
                "data_type": "int",
                "is_primary_key": true
            },
            {
                "name": "order_id",
                "data_type": "int",
                "foreign_key": {
                    "table_name": "Orders",
                    "column_name": "id",
                    "relation_type": "many_to_one"
                }
            },
            {
                "name": "price",
                "data_type": "float",
                "constraints": {
                    "min_value": 0.0,
                    "max_value": 1000.0
                }
            }
        ],
        "total_rows": 210
    }
